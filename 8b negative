ANDS R1,R1,#1<<31 
BHI FOUND 
B LOOP1 
FOUND ADD R2,R2,#1 
B LOOP1 
LOOP1 SUBS R5,R5,#1 
CMP R5,#0
BNE LOOP 
NOP
NOP
