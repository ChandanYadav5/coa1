MOV R5,#3
NXTPASS LDR R0,A
MOV R4,R5
NXTCOMP LDR R2,[R0]
MOV R1,R2
ADD R0,#4
LDR R2,[R0]
CMP R1,R2
BLS NOEXG
STR R1,[R0],#-4
STR R2,[R0],#4
NOEXG SUBS R4,#1
BNE NXTCOMP
SUBS NXTPASS
HERE B HERE
A DCD 0X400002A
NOP
STOP
END

OUTPUT;
I/P:- 
01000000 22000000 33000000 10000000 55000000
0X40000015:- 01000000 10000000 22000000 33000000 55000000
